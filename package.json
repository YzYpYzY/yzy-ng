{
  "name": "yzy-lab",
  "version": "1.6.1",
  "scripts": {
    "adjust-version": "npm-run-all adjust-version:*",
    "adjust-version:lib": "rjp projects/yzy-ng/package.json version $VERSION",
    "adjust-version:main": "rjp package.json version $VERSION",
    "build": "npm run build:theme && npm run build:lib && npm run build:showcase",
    "build:lib": "ng build yzy-ng && npm run copy:readme && npm run copy:theme",
    "build:showcase": "build-storybook -c .storybook -o dist/showcase",
    "build:storybook": "build-storybook",
    "build:theme": "node-sass src/styles/ -o dist/yzy-ng-themes/",
    "copy:readme": "copyfiles ./README.md ./dist/yzy-ng",
    "copy:theme": "copyfiles ./dist/yzy-ng-themes/base-theme.css ./dist/yzy-ng/base-theme.css",
    "e2e": "ng e2e",
    "format:check": "prettier --list-different 'projects/**/*.ts'",
    "format:write": "prettier --write 'projects/**/*.ts'",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "ng": "ng",
    "publish:lib": "npx semantic-release",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/showcase --name=YzYpYzY --email=culot.jerome@gmail.com",
    "start": "npm run start:storybook",
    "start:storybook": "start-storybook -p 6006",
    "start:theme": "node-sass src/styles/ -o dist/yzy-ng-themes/ && node-sass src/styles/ -o dist/yzy-ng-themes/ --watch",
    "start:lib": "ng build yzy-ng --watch",
    "test": "ng test",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@storybook/theming": "^5.3.17",
    "html-loader": "^0.5.5",
    "rxjs": "~6.5.4",
    "to-string-loader": "^1.1.6",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@babel/core": "^7.8.4",
    "@semantic-release/changelog": "^5.0.0",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-notes": "^5.3.17",
    "@storybook/addon-storysource": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/angular": "^5.3.17",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.1.2",
    "copyfiles": "^2.2.0",
    "fibers": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "replace-json-property": "^1.4.3",
    "semantic-release": "^17.0.4",
    "semantic-release-cli": "^5.2.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.38.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/YzYpYzY/yzy-ng.git"
  }
}
