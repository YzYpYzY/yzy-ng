{
  "name": "yzy-lab",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build-theme": "node-sass src/styles/ -o dist/yzy-ng-themes/ && node-sass src/styles/ -o dist/yzy-ng-themes/ --watch",
    "test": "ng test",
    "lint": "ng lint",
    "lintfix": "ng lint --fix",
    "e2e": "ng e2e",
    "storybook": "start-storybook -p 6006",
    "copy:readme": "copyfiles ./README.md ./dist/yzy-ng",
    "build-storybook": "build-storybook",
    "build": "npm run build:lib && npm run build:demo",
    "build:lib": "npm run copy:readme && ng build yzy-ng",
    "build:showcase": "build-storybook -c .storybook -o dist/showcase",
    "publish:showcase": "npx angular-cli-ghpages --dir=./dist/showcase",
    "format:check": "prettier --list-different 'projects/**/*.ts'",
    "format:write": "prettier --write 'projects/**/*.ts'"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@storybook/addon-docs": "^5.3.13",
    "html-loader": "^0.5.5",
    "rxjs": "~6.5.4",
    "to-string-loader": "^1.1.6",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.1",
    "@angular-devkit/build-ng-packagr": "~0.900.1",
    "@angular/cli": "~9.0.1",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@babel/core": "^7.8.4",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/addon-knobs": "^5.3.13",
    "@storybook/addon-links": "^5.3.13",
    "@storybook/addon-notes": "^5.3.13",
    "@storybook/addon-storysource": "^5.3.13",
    "@storybook/addons": "^5.3.13",
    "@storybook/angular": "^5.3.13",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.1.2",
    "fibers": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "ng-packagr": "^9.0.0",
    "node-sass": "^4.13.1",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tsickle": "^0.38.0",
    "tslib": "^1.10.0",
    "tslint": "~5.18.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  }
}
